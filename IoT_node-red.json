    {
        "id": "c492ddc1894077af",
        "type": "function",
        "z": "f35383b4c5a849d4",
        "name": "Split Temp/Hum",
        "func": "if (!msg.payload) return null;\n\n// Force parse if payload is still a string\nif (typeof msg.payload === \"string\") {\n    try {\n        msg.payload = JSON.parse(msg.payload);\n    } catch (e) {\n        node.warn(\"Invalid JSON\");\n        return null;\n    }\n}\n\nconst data = msg.payload.d || msg.payload;\nif (!data) return null;\n\nconst temperature = data.temp ?? data.temperature;\nconst humidity = data.humidity ?? data.hum;\n\nif (typeof temperature === 'undefined' || typeof humidity === 'undefined') {\n    node.warn('Missing temperature or humidity');\n    return null;\n}\n\nreturn [\n    { payload: temperature },\n    { payload: humidity }\n];\n",
        "outputs": 2,
        "timeout": "",
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 780,
        "y": 120,
